
{
    "version": 2,
    "rewrites":  [
        {"source": "/(.*)", "destination": "/"}
      ],
    "builds": [{
      "src": "src/main.js",
      "use": "@vercel/node"
    }],
    "routes": [{
      "src": "/(.*)",
      "dest": "src/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
    ]
    }]
  }